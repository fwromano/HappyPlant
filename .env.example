# Plant Watering Calculator Environment Variables

# PlantNet API Key (Required)
# Get from: https://my.plantnet.org/account/api
PLANTNET_API_KEY=your_plantnet_api_key_here

# Trefle API Key (Optional)
# Get from: https://trefle.io/
TREFLE_API_KEY=your_trefle_api_key_here

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True
SECRET_KEY=your_secret_key_here